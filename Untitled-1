#include "ABlocks_DHT.h"

double temp;
double humidity;
DHT dhtA3(A3,DHT11);

void setup()
{
  	pinMode(A3, INPUT);

	dhtA3.begin();
	Serial.begin(9600);
	Serial.flush();
	while(Serial.available()>0)Serial.read();

	pinMode(A0, INPUT);

	temp = dhtA3.readTemperature();
	humidity = dhtA3.readHumidity();

}


void loop()
{

  	Serial.println(temp);
	println("This is temprature")
  	delay(1000);
  	Serial.println(humidity);
	println("This is Humidity")
  	delay(1000);
  	Serial.println(analogRead(A0));
	println("This is BPM)
  	delay(1000);

}